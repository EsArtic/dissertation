net: "prototxt/localization_GoogleNet/train.prototxt"
test_iter: 500
test_interval: 1000
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
type: "Adam"
stepsize: 10000
display: 20
max_iter: 20000
momentum: 0.9
weight_decay: 0.0002
snapshot: 1000
snapshot_prefix: "snapshots/GoogleNet_localization"
# uncomment the following to default to CPU mode solving
solver_mode: GPU
